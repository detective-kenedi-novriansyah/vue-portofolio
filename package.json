    "babel-jest": "^26.6.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-import": "^1.13.1",
    "core-js": "^3.6.5",
    "csv-loader": "^3.0.3",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.0",
    "iview-loader": "^1.3.0",
    "less": "^3.12.2",
    "less-loader": "^7.0.2",
    "lint-staged": "^9.5.0",
